  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.8.4",
    "babel-plugin-import": "^1.13.8",
    "customize-cra": "^1.0.0",
    "less": "^2.7.3",
    "react": "^16.8.6",
    "react-app-rewired": "^2.0.2-next.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.3.4",
